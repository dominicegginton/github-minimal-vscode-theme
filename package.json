{
  "name": "github-minimal-vscode-theme",
  "displayName": "GitHub Minimal Theme",
  "description": "A minimal GitHub theme for VS Code",
  "version": "0.3.0",
  "preview": true,
  "publisher": "dominicegginton",
  "galleryBanner": {
    "color": "#24292e",
    "theme": "dark"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dominicegginton/github-minimal-vscode-theme.git"
  },
  "bugs": {
    "url": "https://github.com/dominicegginton/github-minimal-vscode-theme/issues"
  },
  "engines": {
    "vscode": "^1.43.0"
  },
  "categories": [
    "Themes"
  ],
  "keywords": [
    "theme",
    "github",
    "minimal",
    "light",
    "dark"
  ],
  "contributes": {
    "themes": [
      {
        "label": "GitHub Minimal Light",
        "uiTheme": "vs",
        "path": "./themes/light.json"
      },
      {
        "label": "Github Minimal Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/dark.json"
      }
    ]
  },
  "author": {
    "name": "Dominic Egginton",
    "email": "dominic.egginton@gmail.com",
    "url": "https://dominicegginton.dev"
  },
  "license": "MIT",
  "homepage": "https://github.com/dominicegginton/github-minimal-vscode-theme#readme",
  "scripts": {
    "dev": "nodemon --watch src src/index.js",
    "build": "node src/index.js",
    "lint": "standard"
  },
  "devDependencies": {
    "@primer/primitives": "^4.3.10",
    "hex-color-opacity": "^0.3.1",
    "nodemon": "^2.0.7",
    "standard": "^16.0.3"
  }
}
